{
  "name": "@carnesen/mathjs-app",
  "description": "A toy calculator web app built on math.js",
  "version": "0.0.1",
  "main": "lib/index.js",
  "scripts": {
    "build:browser:min": "cross-env NODE_ENV=production npm run build:browser",
    "build:browser": "cross-env BABEL_ENV=browser webpack --bail",
    "build:server": "cross-env BABEL_ENV=server babel src --out-dir lib --ignore *-test.js,browser",
    "build:server:watch": "npm run build:server -- --watch",
    "build": "npm run clean && npm run build:server",
    "clean": "rimraf lib dist",
    "lint": "standard 'src/**/*.js' webpack.config.js",
    "lint:fix": "npm run lint -- --fix",
    "start": "node lib/server/start.js",
    "test": "npm run lint && npm run test:server",
    "test:server": "cross-env BABEL_ENV=server NODE_ENV=test mocha --compilers js:babel-register src/server/**/*-test.js src/shared/**/*-test.js"
  },
  "dependencies": {
    "@carnesen/sliced-redux": "0.0.1",
    "@carnesen/util": "0.0.6",
    "bunyan": "1.8.1",
    "cross-env": "3.0.0",
    "express": "4.14.0",
    "mathjs": "3.5.3",
    "morgan": "1.7.0",
    "socket.io": "1.4.8",
    "socket.io-client": "1.4.8",
    "webpack": "1.13.2",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.16.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-es2015-modules-commonjs": "6.16.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "json-loader": "0.5.4",
    "mocha": "3.1.0",
    "rimraf": "2.5.4",
    "standard": "8.3.0",
    "supertest": "^2.0.0",
    "supertest-as-promised": "^4.0.0",
    "webpack-node-externals": "1.4.3"
  },
  "files": [
    "lib"
  ],
  "author": {
    "name": "Chris Arnesen",
    "email": "chris.arnesen@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carnesen/mathjs-app.git"
  },
  "bugs": {
    "url": "https://github.com/carnesen/mathjs-app/issues"
  },
  "homepage": "https://github.com/carnesen/mathjs-app",
  "license": "MIT",
  "standard": {
    "globals": [
      "describe",
      "it"
    ]
  }
}
